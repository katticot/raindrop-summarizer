{
  "name": "raindrop-video-summarizer",
  "version": "1.0.0",
  "description": "AI-powered video summarization tool for Raindrop.io bookmarks",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "web": "next dev",
    "web-dev": "next dev",
    "web-restart": "pkill -f 'next dev' || true && npm run web",
